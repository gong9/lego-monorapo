{
    "source": {
        "include": ["src/"],
        "includePattern": ".js$",
        "exclude": ["src/lib"]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "opts": {
        "template": "node_modules/minami",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true
    }
}